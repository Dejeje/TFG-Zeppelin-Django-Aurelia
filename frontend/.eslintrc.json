{
  "root": true,
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": ["@babel/plugin-proposal-decorators"],
  "rules": {
    "no-unused-vars": 0,
    "no-prototype-builtins": 0,
    "no-console": 0,
    "getter-return": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}